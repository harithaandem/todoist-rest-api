{
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "lines": 95,
  "statements": 95,
  "functions": 95,
  "branches": 95,
  "reporter": [
    "text",
    "lcov"
  ],
  "all": true,
  "check-coverage": true,
  "report-dir": "./coverage",
  "cache": false
}
